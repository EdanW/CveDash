{
  "name": "cvedash",
  "version": "1.0.0",
  "description": "CVE Dashboard - A TypeScript application for processing and analyzing CVE data from NVD feeds",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "server": "ts-node src/index.ts",
    "download:recent": "ts-node src/scripts/downloadRecentCves.ts",
    "test:recent": "ts-node src/scripts/testRecentCves.ts",
    "test:nvd": "ts-node src/test-nvd.ts",
    "test:final": "ts-node src/test-final.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  }
}
